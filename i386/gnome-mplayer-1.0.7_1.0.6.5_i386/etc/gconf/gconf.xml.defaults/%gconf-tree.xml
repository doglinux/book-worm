<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="apps">
			<dir name="gnome-mplayer">
				<dir name="preferences">
					<entry name="enable-nautilus-plugin" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Enable Audio/Video tab in nautilus">
							<default type="bool" value="true"/>
							<longdesc>Enable Audio/Video tab in nautilus</longdesc>
						</local_schema>
					</entry>
					<entry name="enable-global-menu" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Set to true when global menus are being used">
							<default type="bool" value="false"/>
							<longdesc>Set to true when global menus are being used</longdesc>
						</local_schema>
					</entry>
					<entry name="show-controls" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Show controls">
							<default type="bool" value="true"/>
							<longdesc>Show controls</longdesc>
						</local_schema>
					</entry>
					<entry name="use-xscrnsaver" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Set to TRUE if you want to use xscrnsaver controls over gnome-power-manager">
							<default type="bool" value="false"/>
							<longdesc>When enabled directly tells the xscreensaver to inhibit itself, otherwise uses gnome-power-manager</longdesc>
						</local_schema>
					</entry>
					<entry name="use-defaultpl" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Set to TRUE if you want to save and load the default playlist">
							<default type="bool" value="false"/>
							<longdesc>Will load and save the playlist when the program runs</longdesc>
						</local_schema>
					</entry>
					<entry name="show-status-icon" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Place status icon in the tray">
							<default type="bool" value="true"/>
							<longdesc>Place status icon in the tray</longdesc>
						</local_schema>
					</entry>
					<entry name="show-subtitles" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Show subtitles by default">
							<default type="bool" value="true"/>
							<longdesc>Load and display subtitles by default when media contains them</longdesc>
						</local_schema>
					</entry>
					<entry name="show-notification" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Show notification via libnotify">
							<default type="bool" value="true"/>
							<longdesc>Show notification via libnotify</longdesc>
						</local_schema>
					</entry>
					<entry name="use-mediakeys" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Use Media Keys">
							<default type="bool" value="true"/>
							<longdesc>Process the media key events from Gnome</longdesc>
						</local_schema>
					</entry>
					<entry name="keep-on-top" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Remember location on screen">
							<default type="bool" value="false"/>
							<longdesc>Remember location on screen</longdesc>
						</local_schema>
					</entry>
					<entry name="tracker-position" mtime="1336432967" type="schema" stype="int" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Tracker thumb position">
							<default type="int" value="3"/>
							<longdesc>Setting for tracker thumb position</longdesc>
						</local_schema>
					</entry>
					<entry name="window-y" mtime="1336432967" type="schema" stype="int" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Window top corner (y)">
							<default type="int" value="0"/>
							<longdesc>Window top corner (y)</longdesc>
						</local_schema>
					</entry>
					<entry name="window-x" mtime="1336432967" type="schema" stype="int" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Window top corner (x)">
							<default type="int" value="0"/>
							<longdesc>Window top corner (x)</longdesc>
						</local_schema>
					</entry>
					<entry name="audio-channels" mtime="1336432967" type="schema" stype="int" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Default Audio Channels (internal value)">
							<default type="int" value="0"/>
							<longdesc>Default Audio Channels to output, this is an internal value to the application</longdesc>
						</local_schema>
					</entry>
					<entry name="subtitle-fuzziness" mtime="1336432967" type="schema" stype="int" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Subtitle Loading">
							<default type="int" value="0"/>
							<longdesc>Technique for loading external subtitle files</longdesc>
						</local_schema>
					</entry>
					<entry name="volume" mtime="1336432967" type="schema" stype="int" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Default Volume">
							<default type="int" value="100"/>
							<longdesc>Default volume to use for mplayer, values of 0 or 100 allow usage of setting from ALSA</longdesc>
						</local_schema>
					</entry>
					<entry name="remember-loc" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Remember location on screen">
							<default type="bool" value="false"/>
							<longdesc>Remember location on screen</longdesc>
						</local_schema>
					</entry>
					<entry name="extraopts" mtime="1336432967" type="schema" stype="string" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Extra options to pass to mplayer">
							<default type="string">
								<stringvalue></stringvalue>
							</default>
							<longdesc>Extra options to pass to mplayer</longdesc>
						</local_schema>
					</entry>
					<entry name="bring-to-front" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Bring window to front in single instance mode">
							<default type="bool" value="false"/>
							<longdesc>Bting window to front in single instance mode</longdesc>
						</local_schema>
					</entry>
					<entry name="replace-and-play" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Replace currently playing media in single instance mode">
							<default type="bool" value="false"/>
							<longdesc>Replace currently playing media in single instance mode</longdesc>
						</local_schema>
					</entry>
					<entry name="single-instance" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Only allow one instance of the player to run">
							<default type="bool" value="false"/>
							<longdesc>Only allow one instance of the player to run</longdesc>
						</local_schema>
					</entry>
					<entry name="subtitle-scale" mtime="1336432967" type="schema" stype="float" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Default font scaling to use for subtitles">
							<default type="float" value="1"/>
							<longdesc>Default font scaling to use for subtitles</longdesc>
						</local_schema>
					</entry>
					<entry name="subtitle-color" mtime="1336432967" type="schema" stype="string" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Default color to use for subtitles">
							<default type="string">
								<stringvalue></stringvalue>
							</default>
							<longdesc>Default color to use for subtitles</longdesc>
						</local_schema>
					</entry>
					<entry name="subtitle-font" mtime="1336432967" type="schema" stype="string" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Default font to use for subtitles">
							<default type="string">
								<stringvalue></stringvalue>
							</default>
							<longdesc>Default font to use for subtitles</longdesc>
						</local_schema>
					</entry>
					<entry name="subtitle-codepage" mtime="1336432967" type="schema" stype="string" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Codepage to use when loading subtitle files">
							<default type="string">
								<stringvalue></stringvalue>
							</default>
							<longdesc>Codepage to use when loading subtitle files</longdesc>
						</local_schema>
					</entry>
					<entry name="metadata-codepage" mtime="1336432967" type="schema" stype="string" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Codepage to use when loading the metadata of files">
							<default type="string">
								<stringvalue></stringvalue>
							</default>
							<longdesc>Codepage to use when loading the metadata of files</longdesc>
						</local_schema>
					</entry>
					<entry name="softvol" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Have mplayer use the softvol option">
							<default type="bool" value="false"/>
							<longdesc>When set to TRUE the softvol option is passed to mplayer, useful for machines without pulseaudio</longdesc>
						</local_schema>
					</entry>
					<entry name="force-cache" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Force the cache setting">
							<default type="bool" value="false"/>
							<longdesc>When set to TRUE the cache value is always used, even for streaming media</longdesc>
						</local_schema>
					</entry>
					<entry name="disable-cover-art-fetch" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Disable Fetch of Cover Art">
							<default type="bool" value="false"/>
							<longdesc>When set to TRUE cached covers are loaded, but new ones are not retrieved</longdesc>
						</local_schema>
					</entry>
					<entry name="auto-hide-timeout" mtime="1336432967" type="schema" stype="int" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Time in seconds before controls are hidden">
							<default type="int" value="3"/>
							<longdesc>Time in seconds before controls are hidden when in fullscreen mode values less than 1 disable auto hide</longdesc>
						</local_schema>
					</entry>
					<entry name="disable-animation" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Disable Animation">
							<default type="bool" value="false"/>
							<longdesc>Disable animation of fullscreen control bar sliding away</longdesc>
						</local_schema>
					</entry>
					<entry name="disable-pause-on-click" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Disable Pause on Click action">
							<default type="bool" value="true"/>
							<longdesc>When set to TRUE the ability to pause on mouse click is disabled</longdesc>
						</local_schema>
					</entry>
					<entry name="disable-deinterlace" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Disable Deinterlace filter">
							<default type="bool" value="true"/>
							<longdesc>When set to TRUE the usage of the deinterlace filter is disabled</longdesc>
						</local_schema>
					</entry>
					<entry name="disable-embeddedfonts" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Disable Embedded Fonts">
							<default type="bool" value="false"/>
							<longdesc>When set to TRUE Subtitle Fonts in the media file are disabled</longdesc>
						</local_schema>
					</entry>
					<entry name="disable-ass" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Disable ASS">
							<default type="bool" value="false"/>
							<longdesc>When set to TRUE the Advanced Substation Alpha (ASS) subtitles are disabled</longdesc>
						</local_schema>
					</entry>
					<entry name="disable-contextmenu" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Disable Context Menu">
							<default type="bool" value="false"/>
							<longdesc>When set to TRUE the right-click context menu is disabled</longdesc>
						</local_schema>
					</entry>
					<entry name="disable-framedrop" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Disable Mplayer frame drop option">
							<default type="bool" value="false"/>
							<longdesc>When set to TRUE all mplayer is called without the -framedrop option</longdesc>
						</local_schema>
					</entry>
					<entry name="disable-fullscreen" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Disable Fullscreen Options">
							<default type="bool" value="false"/>
							<longdesc>When set to TRUE all fullscreen menu options and keyboard options are disabled</longdesc>
						</local_schema>
					</entry>
					<entry name="show-details" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Show Playlist">
							<default type="bool" value="false"/>
							<longdesc>Make the details visible when gnome-mplayer is started</longdesc>
						</local_schema>
					</entry>
					<entry name="show-playlist" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Show Playlist">
							<default type="bool" value="false"/>
							<longdesc>Make the playlist visible when gnome-mplayer is started</longdesc>
						</local_schema>
					</entry>
					<entry name="vertical" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Vertical">
							<default type="bool" value="false"/>
							<longdesc>Use Veritical Layout</longdesc>
						</local_schema>
					</entry>
					<entry name="last-dir" mtime="1336432967" type="schema" stype="string" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Last directory">
							<default type="string">
								<stringvalue></stringvalue>
							</default>
							<longdesc>Last visited direcory with open dialog</longdesc>
						</local_schema>
					</entry>
					<entry name="verbose" mtime="1336432967" type="schema" stype="int" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Verbose Messages">
							<default type="int" value="0"/>
							<longdesc>Set Verbose debug messages to be default</longdesc>
						</local_schema>
					</entry>
					<entry name="fullscreen" mtime="1336432967" type="schema" stype="bool" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="start in fullscreen as soon as possible">
							<default type="bool" value="false"/>
							<longdesc>switch to fullscreen mode on video load</longdesc>
						</local_schema>
					</entry>
					<entry name="pp-level" mtime="1336432967" type="schema" stype="int" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Postprocessing Level">
							<default type="int" value="0"/>
							<longdesc>Value for the Postprocessing Setting</longdesc>
						</local_schema>
					</entry>
					<entry name="osd-level" mtime="1336432967" type="schema" stype="int" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="OSD Level">
							<default type="int" value="0"/>
							<longdesc>Value for the On Screen Display Setting</longdesc>
						</local_schema>
					</entry>
					<entry name="alsa-mixer" mtime="1336432967" type="schema" stype="string" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Default mixer to use">
							<default type="string">
								<stringvalue></stringvalue>
							</default>
							<longdesc>Default mixer</longdesc>
						</local_schema>
					</entry>
					<entry name="plugin-video-cache-size" mtime="1336432967" type="schema" stype="int" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Cache size">
							<default type="int" value="2048"/>
							<longdesc>The memory cache size for playing</longdesc>
						</local_schema>
					</entry>
					<entry name="plugin-audio-cache-size" mtime="1336432967" type="schema" stype="int" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Cache size">
							<default type="int" value="128"/>
							<longdesc>The memory cache size for playing</longdesc>
						</local_schema>
					</entry>
					<entry name="cache-size" mtime="1336432967" type="schema" stype="int" owner="gnome-mplayer">
						<local_schema locale="C" short_desc="Cache size">
							<default type="int" value="2000"/>
							<longdesc>The memory cache size for playing</longdesc>
						</local_schema>
					</entry>
				</dir>
			</dir>
		</dir>
	</dir>
